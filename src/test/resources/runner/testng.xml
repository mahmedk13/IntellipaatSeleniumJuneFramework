<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
<parameter name="grid" value="false"/>

<listeners>

<listener class-name="extentlisteners.ExtentListeners"></listener>

</listeners>


  <test thread-count="5" name="Chrome Test">
    <parameter name="browser" value="chrome"/>
  <parameter name="url" value="https://www.expedia.com/"/>
    <classes>
      
      <class name="tests.FlightSearchTest"/>
      <class name="tests.HotelSearchTest"/>
      
      
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Firefox Test">
    <parameter name="browser" value="firefox"/>
  <parameter name="url" value="https://www.expedia.com/"/>
    <classes>
      
      <class name="tests.FlightSearchTest"/>
      <class name="tests.HotelSearchTest"/>
      
      
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
